 <thead>
    <tr>
      <th>ジャンル</th>
      <th>状態</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% genres.each do |genre| %>
      <tr>
        <td>
          <%= genre.name %>
        </td>
        <td>
          <% if genre.is_active == true %>
            有効
          <% else %>
            無効
          <% end %>
        </td>
        <td>
          <%= link_to '編集する', edit_admins_genre_path(genre.id), class: '' %>
        </td>
      </tr>
    <% end %>
  </tbody>