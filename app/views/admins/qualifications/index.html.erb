<table class=''>
  <thead>
    <tr>
      <th>資格名</th>
      <th>ジャンル</th>
      <th>概要</th>
    </tr>
  </thead>

  <tbody>
    <% @qualifications.each do |qualification| %>
      <tr>
        <td>
          <%= link_to qualification.name, admins_qualification_path(qualification) %>
        </td>
        <td><%= qualification.genre.name %></td>
        <td><%= qualification.body %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%#= link_to user_path(book.user) do %>
  <%#= attachment_image_tag(book.user, :profile_image, :fill, 50, 50, fallback: "no-image-icon.jpg") %>
<%# end %>